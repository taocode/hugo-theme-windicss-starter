{
  "name": "hugo-theme-windicss-starter",
  "version": "1.0.0",
  "description": "A Hugo starter theme with UnoCSS",
  "main": "index.js",
  "scripts": {
    "uno": "unocss \"layouts/**/*.html\" \"content/**/*.md\" \"assets/css/modules/site.css\" \"themes/hugo-theme-windicss-starter/layouts/**/*.html\" -o \"assets/css/modules/_uno.css\"",
    "uno:dev": "unocss \"layouts/**/*.html\" \"content/**/*.md\" \"assets/css/modules/site.css\" \"themes/hugo-theme-windicss-starter/layouts/**/*.html\" --watch -o \"assets/css/modules/_uno.css\"",
    "hugo:dev": "hugo server -DF",
    "hugo": "hugo --gc --minify",
    "dev": "run-p uno:dev hugo:dev",
    "prebuild": "[[ $CI = true ]] && npx pnpm install --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "build": "run-s uno hugo",
    "example": "run-p uno:example hugo:example",
    "uno:example": "unocss \"../layouts/**/*.html\" --watch -o \"../assets/css/modules/_uno.css\"",
    "hugo:example": "hugo server -s ./ -DF --disableFastRender --themesDir=\"../../\"",
    "build:example": "run-s uno:build:example hugo:build:example",
    "uno:build:example": "unocss \"../layouts/**/*.html\" -o \"../assets/css/modules/_uno.css\"",
    "hugo:build:example": "hugo --gc -s ./ --themesDir=\"../../\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taocode/hugo-theme-windi-starter.git"
  },
  "keywords": [
    "hugo",
    "windicss",
    "unocss",
    "template",
    "starter"
  ],
  "author": "taocode",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/taocode/hugo-theme-windicss-starter/issues"
  },
  "homepage": "https://github.com/taocode/hugo-theme-windicss-starter#readme",
  "devDependencies": {
    "@iconify-json/fe": "^1.1.6",
    "@unocss/cli": "^0.51.8",
    "npm-run-all": "^4.1.5",
    "unocss": "^0.51.8",
    "vite": "^4.3.3"
  }
}
